<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/relnew"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.sam.leceladon_managing_10.NewActivity">

    <TextView
        android:id="@+id/textViewprenom"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_above="@+id/editTextprenom"
        android:layout_alignParentStart="true"
        android:text="Prenom" />

    <EditText
        android:id="@+id/editTextprenom"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_above="@+id/textViewnom"
        android:layout_alignParentStart="true"
        android:ems="10"
        android:inputType="textPersonName" />

    <EditText
        android:id="@+id/editTextnom"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"

        android:layout_above="@+id/Adress"
        android:layout_alignParentStart="true"
        android:inputType="textPersonName" />

    <TextView
        android:id="@+id/textViewnom"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_above="@+id/editTextnom"
        android:layout_alignParentStart="true"
        android:text="Nom" />

    <EditText
        android:id="@+id/etadresse"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"

        android:layout_above="@+id/txttel"
        android:layout_alignParentStart="true"
        android:inputType="textPostalAddress" />

    <TextView
        android:id="@+id/Adress"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_above="@+id/etadresse"
        android:layout_alignParentStart="true"
        android:text="Adresse" />

    <EditText
        android:id="@+id/telphone"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"

        android:layout_above="@+id/textView4"
        android:layout_alignParentStart="true"
        android:inputType="number" />

    <TextView
        android:id="@+id/txttel"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_above="@+id/telphone"
        android:layout_alignParentStart="true"
        android:text="Telphone" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_above="@+id/email"
        android:layout_alignParentStart="true"
        android:text="Email" />

    <EditText
        android:id="@+id/email"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"

        android:layout_above="@+id/textViewmotdepasse"
        android:layout_alignParentStart="true" />

    <EditText
        android:id="@+id/psw"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"

        android:layout_alignParentBottom="true"
        android:layout_alignParentStart="true"
        android:layout_marginBottom="106dp"
        android:inputType="textPassword" />

    <TextView
        android:id="@+id/textViewmotdepasse"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_above="@+id/psw"
        android:layout_alignParentStart="true"
        android:text="Mot de passe" />

    <ImageView
        android:id="@+id/imageView13"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginTop="44dp"
        app:srcCompat="@mipmap/a92589064" />

    <Button
        android:id="@+id/inscrire"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentStart="true"
        android:layout_marginBottom="20dp"
        android:text="S'inscrire" />


</RelativeLayout>

